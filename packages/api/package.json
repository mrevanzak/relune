{
  "name": "@relune/api",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./context": {
      "default": "./src/context.ts"
    },
    "./routers": {
      "default": "./src/routers/index.ts"
    },
    "./routers/*": {
      "default": "./src/routers/*.ts"
    },
    "./models/*": {
      "default": "./src/models/*.ts"
    },
    "./services/*": {
      "default": "./src/services/*.ts"
    },
    "./middleware/*": {
      "default": "./src/middleware/*.ts"
    }
  },
  "scripts": {},
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@orpc/client": "catalog:",
    "@orpc/openapi": "catalog:",
    "@orpc/server": "catalog:",
    "@orpc/zod": "catalog:",
    "@relune/db": "workspace:*",
    "@relune/env": "workspace:*",
    "@supabase/supabase-js": "catalog:",
    "ai": "^6.0.3",
    "dotenv": "catalog:",
    "drizzle-orm": "catalog:",
    "file-type": "^21.2.0",
    "jszip": "^3.10.1",
    "node-av": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@relune/config": "workspace:*",
    "@types/pg": "catalog:"
  },
  "trustedDependencies": [
    "node-av"
  ]
}
