{
  "name": "@relune/api",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./context": {
      "default": "./src/context.ts"
    },
    "./routers": {
      "default": "./src/routers/index.ts"
    },
    "./routers/*": {
      "default": "./src/routers/*.ts"
    },
    "./models/*": {
      "default": "./src/models/*.ts"
    },
    "./services/*": {
      "default": "./src/services/*.ts"
    },
    "./middleware/*": {
      "default": "./src/middleware/*.ts"
    },
    "./lib/logger": {
      "default": "./src/lib/logger.ts"
    }
  },
  "scripts": {},
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@orpc/client": "catalog:",
    "@orpc/openapi": "catalog:",
    "@orpc/server": "catalog:",
    "@orpc/zod": "catalog:",
    "@relune/db": "workspace:*",
    "@relune/env": "workspace:*",
    "@supabase/supabase-js": "catalog:",
    "ai": "^6.0.3",
    "dotenv": "catalog:",
    "drizzle-orm": "catalog:",
    "file-type": "^21.2.0",
    "jszip": "^3.10.1",
    "node-av": "catalog:",
    "zod": "catalog:",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@relune/config": "workspace:*"
  },
  "trustedDependencies": [
    "node-av",
    "@seydx/node-av-darwin-arm64",
    "@seydx/node-av-darwin-x64",
    "@seydx/node-av-linux-arm64",
    "@seydx/node-av-linux-x64",
    "@seydx/node-av-win32-arm64-mingw",
    "@seydx/node-av-win32-arm64-msvc",
    "@seydx/node-av-win32-x64-mingw",
    "@seydx/node-av-win32-x64-msvc"
  ]
}
