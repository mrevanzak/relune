{
  "name": "@relune/api",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./context": {
      "default": "./src/context.ts"
    },
    "./routers": {
      "default": "./src/routers/index.ts"
    },
    "./routers/*": {
      "default": "./src/routers/*.ts"
    },
    "./models/*": {
      "default": "./src/models/*.ts"
    },
    "./services/*": {
      "default": "./src/services/*.ts"
    },
    "./middleware/*": {
      "default": "./src/middleware/*.ts"
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@orpc/client": "^1.12.2",
    "@orpc/openapi": "^1.12.2",
    "@orpc/server": "^1.12.2",
    "@orpc/zod": "^1.12.2",
    "@relune/db": "workspace:*",
    "@relune/env": "workspace:*",
    "@supabase/supabase-js": "^2.49.8",
    "ai": "^6.0.3",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.45.1",
    "file-type": "^21.2.0",
    "jszip": "^3.10.1",
    "node-av": "^5.0.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@relune/config": "workspace:*",
    "@types/pg": "^8.15.6",
    "typescript": "^5"
  },
  "trustedDependencies": [
    "node-av"
  ]
}
