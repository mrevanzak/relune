{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.js"
  },
  "dependencies": {
    "elysia": "^1.3.21",
    "@elysiajs/cors": "^1.3.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4"
  },
  "trustedDependencies": [
    "supabase"
  ]
}
